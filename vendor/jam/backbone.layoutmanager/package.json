{
  "author": "Tim Branyen (@tbranyen)",
  "name": "backbone.layoutmanager",
  "description": "A manager for templates & layouts in Backbone.js",

  "version": "0.7.3",

  "homepage": "http://tbranyen.github.com/backbone.layoutmanager/",

  "repository": {
    "type": "git",
    "url": "git://github.com/tbranyen/backbone.layoutmanager.git"
  },

  "main": "node/index",

  "engines": {
    "node": ">=0.8"
  },

  "dependencies": {
    "backbone": "0.9.2",
    "underscore.deferred": "0.2.0",
    "cheerio": "0.10.3",
    "underscore": "1.4.2"
  },

  "devDependencies": {
    "grunt": "~0.3.15"
  },

  "scripts": {
    "test": "grunt"
  },

  "jam": {
    "dependencies": {
      "lodash": "*",
      "backbone": "*",
      "jquery": "*"
    },

    "include": ["backbone.layoutmanager.js"],

    "main": "backbone.layoutmanager.js",

    "shim": {
      "deps": ["jquery", "backbone", "lodash"],
      "exports": "Backbone.LayoutManager"
    }
  }
}
